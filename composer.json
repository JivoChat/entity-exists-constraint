{
    "name": "jivochat/entity-exists-constraint",
    "description": "Symfony validator for checking if Entity Exist",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "symfony/validator": "^3.4 || ^4.4",
        "doctrine/orm": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "JivoChat\\Validator\\Constraint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\JivoChat\\Validator\\Constraint\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    }
}
